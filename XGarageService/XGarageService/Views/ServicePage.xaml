             xmlns:d="http://xamarin.com/schemas/2014/forms/design"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             mc:Ignorable="d"
             Title="Service"
             x:Class="XGarageService.Views.ServicePage">
    <ContentPage.Content>
        <StackLayout>
            <StackLayout Orientation="Horizontal" Margin="10,10" HorizontalOptions="Fill">
                <Label Text="Car Reg" FontSize="Medium" VerticalOptions="Center"/>
                <Button Text="Submin"  FontSize="Large" TextColor="Blue" FontAttributes="Bold" BackgroundColor="Transparent" HorizontalOptions="Start" 
                        Clicked="Button_SubmitOrder"/>
            </StackLayout>
        </StackLayout>
    </ContentPage.Content>
</ContentPage>